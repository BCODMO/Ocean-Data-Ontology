@prefix : <http://ocean-data.org/schema/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://ocean-data.org/schema/> .

<http://ocean-data.org/schema/> rdf:type owl:Ontology ;
                                 owl:versionIRI <http://ocean-data.org/schema/> ;
                                 <http://purl.org/dc/elements/1.1/publisher> "Biological and Chemical Oceanography Data Management Office (BCO-DMO)"@en-us ;
                                 <http://purl.org/dc/terms/created> "2012-01-01"@en-us ;
                                 <http://purl.org/dc/terms/creator> <http://orcid.org/0000-0003-4486-9448> ;
                                 <http://purl.org/dc/terms/issued> "2019-03-27"^^xsd:date ;
                                 <http://purl.org/dc/terms/modified> "2019-03-27"^^xsd:date ;
                                 <http://purl.org/dc/terms/publisher> <http://lod.bco-dmo.org/id/affiliation/191> ;
                                 <http://purl.org/dc/terms/source> "https://github.com/BCODMO/Ocean-Data-Ontology"^^<xsd:anyURI> ;
                                 <http://purl.org/dc/terms/title> "Ocean Data Ontology"@en-us ;
                                 <http://purl.org/vocab/vann/preferredNamespaceUri> "http://ocean-data.org/schema/"^^xsd:anyURI ,
                                                                                    "odo" ;
                                 rdfs:comment "An ontology for describing metadata about oceanographic data."@en-us ;
                                 rdfs:label "Ocean Data Ontology"@en-us ;
                                 owl:versionInfo "0.9.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/publisher
<http://purl.org/dc/elements/1.1/publisher> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
<http://purl.org/dc/terms/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/issued
<http://purl.org/dc/terms/issued> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
<http://purl.org/dc/terms/modified> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/publisher
<http://purl.org/dc/terms/publisher> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/source
<http://purl.org/dc/terms/source> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#About
rdf:About rdf:type owl:AnnotationProperty .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#Resource
rdf:Resource rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#defintion
<http://www.w3.org/2004/02/skos/core#defintion> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#related
<http://www.w3.org/2004/02/skos/core#related> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.opengis.net/ont/geosparql#wktLiteral
<http://www.opengis.net/ont/geosparql#wktLiteral> rdf:type rdfs:Datatype .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#HTML
rdf:HTML rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://ocean-data.org/schema/archiver
:archiver rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :ArchiveRecord ;
          rdfs:range :Organization ;
          rdfs:comment "The Organization acting as the archiver of some research product."@en-us ;
          rdfs:label "archiver"@en-us .


###  http://ocean-data.org/schema/archives
:archives rdf:type owl:ObjectProperty ;
          owl:inverseOf :isArchivedBy ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :ArchiveRecord ;
          rdfs:range :CreativeWork ;
          rdfs:comment "The CreativeWork archived by some Organization described in this record."@en-us ;
          rdfs:label "archives"@en-us .


###  http://ocean-data.org/schema/checksumAlgorithm
:checksumAlgorithm rdf:type owl:ObjectProperty ;
                   rdfs:domain :DigitalDocument ;
                   rdfs:range owl:Thing ;
                   rdfs:comment "The algorithm of some cryptographic hash"@en-us ;
                   rdfs:label "checksumAlgorithm"@en-us .


###  http://ocean-data.org/schema/crossrefRelation
:crossrefRelation rdf:type owl:ObjectProperty ;
                  rdfs:domain :CreativeWorkRelationType ;
                  rdfs:range :CrossrefRelation ;
                  rdfs:comment "The type of relationship defined by Crossref."@en-us ;
                  rdfs:label "crossrefRelation"@en-us .


###  http://ocean-data.org/schema/currentState
:currentState rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Dataset ;
              rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty <http://www.w3.org/2004/02/skos/core#member> ;
                                                  owl:allValuesFrom :CurrentState
                                                ]
                                              ) ;
                           rdf:type owl:Class
                         ] ;
              rdfs:comment "A term describing the current state of the dataset."@en-us ;
              rdfs:label "currentState"@en-us .


###  http://ocean-data.org/schema/dataFile
:dataFile rdf:type owl:ObjectProperty ;
          owl:inverseOf :serializes ;
          rdfs:domain :Dataset ;
          rdfs:range :DataFile ;
          rdfs:comment "A data file recording the observations and measurements for a dataset."@en-us ;
          rdfs:label "dataFile"@en-us .


###  http://ocean-data.org/schema/datasetType
:datasetType rdf:type owl:ObjectProperty ;
             rdfs:domain :Dataset ;
             rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty <http://www.w3.org/2004/02/skos/core#member> ;
                                                 owl:allValuesFrom :DatasetType
                                               ]
                                             ) ;
                          rdf:type owl:Class
                        ] ;
             rdfs:comment "The type of dataset."@en-us ;
             rdfs:label "datasetType"@en-us .


###  http://ocean-data.org/schema/forOrganization
:forOrganization rdf:type owl:ObjectProperty ;
                 rdfs:domain :Role ;
                 rdfs:range :Organization .


###  http://ocean-data.org/schema/fromCollection
:fromCollection rdf:type owl:ObjectProperty ;
                owl:inverseOf :hasInventory ;
                rdfs:domain :Dataset ;
                rdfs:range :DatasetCollection .


###  http://ocean-data.org/schema/fromDeployment
:fromDeployment rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :DeploymentDataset ;
                rdfs:range :Deployment ;
                rdfs:comment " The deployment from which this dataset was collected."@en-us ;
                rdfs:label "fromDeployment"@en-us .


###  http://ocean-data.org/schema/fromInstrument
:fromInstrument rdf:type owl:ObjectProperty ;
                rdfs:domain :Dataset ;
                rdfs:range :Instrument ;
                rdfs:comment "This dataset was collected from an instrument that is located on the platform during this deployment."@en-us ;
                rdfs:label "fromInstrument"@en-us .


###  http://ocean-data.org/schema/funded
:funded rdf:type owl:ObjectProperty ;
        owl:inverseOf :fundedBy ;
        rdfs:domain :Award ;
        rdfs:range [ rdf:type owl:Class ;
                     owl:unionOf ( :CreativeWork
                                   :ResearchActivity
                                 )
                   ] ;
        rdfs:comment "Funded the research actvity or product."@en-us ;
        rdfs:label "funded"@en-us .


###  http://ocean-data.org/schema/fundedBy
:fundedBy rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :CreativeWork
                                      :ResearchActivity
                                    )
                      ] ;
          rdfs:range :Award ;
          rdfs:comment "Specifies how a research activity or product was funded"@en-us ;
          rdfs:label "fundedBy"@en-us .


###  http://ocean-data.org/schema/generatedWork
:generatedWork rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://www.w3.org/ns/prov#generated> ;
               rdfs:domain :ResearchActivity ;
               rdfs:range :CreativeWork ;
               rdfs:comment "A creative work, research product, that was generated due to some research activity."@en-us ;
               rdfs:label "generatedWork"@en-us .


###  http://ocean-data.org/schema/hasAgentWithRole
:hasAgentWithRole rdf:type owl:ObjectProperty ;
                  owl:inverseOf :roleAbout ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Award
                                              :Dataset
                                              :Deployment
                                              :Organization
                                              :ResearchActivity
                                            )
                              ] ;
                  rdfs:range :Role ;
                  rdfs:comment "Specifies role some agent played on a certain research activity or product."@en-us ;
                  rdfs:label "hasAgentWithRole"@en-us .


###  http://ocean-data.org/schema/hasAuthoritativePublisher
:hasAuthoritativePublisher rdf:type owl:ObjectProperty ;
                           rdfs:domain :CreativeWork ;
                           rdfs:range :Agent ;
                           rdfs:comment "The Agent that is the authoritative source for some Creative Work. Was using dcterms:publisher."@en-us ;
                           rdfs:label "hasAuthoritativePublisher"@en-us .


###  http://ocean-data.org/schema/hasAward
:hasAward rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Dataset
                                      :FundingSource
                                      :Project
                                    )
                      ] ;
          rdfs:range :Award ;
          rdfs:comment "Whether an activity (Project) or Entity (Dataset or Funding Source) is attached to an award. This should probably be brokenn into different terms - :Dataset 'fundedBy' :Award; :FundingSource 'provesAward' :Award; :Award 'fundsActivity' :Project"@en-us ;
          rdfs:label "hasAward"@en-us ;
          owl:deprecated "true"^^xsd:boolean ;
          <http://www.w3.org/2004/02/skos/core#related> :fundedBy ,
                                                        :providesAward .


###  http://ocean-data.org/schema/hasCoordinatedDeployment
:hasCoordinatedDeployment rdf:type owl:ObjectProperty ;
                          rdfs:domain :Deployment ;
                          rdfs:range :Deployment ;
                          rdfs:comment "A disparate deployment that coordinated in conjuction with this subject for data collection"@en-us ;
                          rdfs:label "hasCoordinatedDeployment"@en-us .


###  http://ocean-data.org/schema/hasCruiseTrajectory
:hasCruiseTrajectory rdf:type owl:ObjectProperty ;
                     rdfs:domain :Cruise ;
                     rdfs:range <http://www.opengis.net/ont/geosparql#Feature> ;
                     rdfs:comment "The set of ordered coordinates forming a geometry (usually MultiPoint) representing the path taken by a Cruise."@en-us ;
                     rdfs:label "hasCruiseTrajectory"@en-us .


###  http://ocean-data.org/schema/hasDataset
:hasDataset rdf:type owl:ObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Award
                                        :DeploymentDataset
                                        :Project
                                      )
                        ] ;
            rdfs:range :Dataset .


###  http://ocean-data.org/schema/hasDeploymentDates
:hasDeploymentDates rdf:type owl:ObjectProperty ;
                    rdfs:domain :Deployment ;
                    rdfs:range <http://www.w3.org/2006/time#TemporalEntity> ;
                    rdfs:comment "A date range for the start and end of a deployment"@en-us ;
                    rdfs:label "hasDeploymentDates"@en-us .


###  http://ocean-data.org/schema/hasFundingSource
:hasFundingSource rdf:type owl:ObjectProperty ;
                  rdfs:domain :Award ;
                  rdfs:range :Organization ;
                  rdfs:comment "The source of funding for an award"@en-us ;
                  rdfs:label "hasFundingSource"@en-us .


###  http://ocean-data.org/schema/hasIdentifier
:hasIdentifier rdf:type owl:ObjectProperty ;
               rdfs:domain :CreativeWork ;
               rdfs:range :Identifier ;
               rdfs:comment "An identifier for a Creative Work."@en-us .


###  http://ocean-data.org/schema/hasInventory
:hasInventory rdf:type owl:ObjectProperty ;
              rdfs:domain :DatasetCollection ;
              rdfs:range :Dataset ;
              rdfs:comment "Defines the Datasets that are part of this collection"@en-us ,
                           "fix" ;
              rdfs:label "hasInventory"@en-us .


###  http://ocean-data.org/schema/hasParentProgram
:hasParentProgram rdf:type owl:ObjectProperty ;
                  owl:inverseOf :hasProject ;
                  rdfs:domain :Project ;
                  rdfs:range :Program ;
                  rdfs:comment "The Program which made the Project possible"@en-us ;
                  rdfs:label "hasParentProgram"@en-us .


###  http://ocean-data.org/schema/hasPlatformTitle
:hasPlatformTitle rdf:type owl:ObjectProperty ;
                  rdfs:domain :Platform ;
                  rdfs:range :PlatformTitle ;
                  rdfs:comment "The title of a Platform"@en-us ;
                  rdfs:label "hasPlatformTitle"@en-us .


###  http://ocean-data.org/schema/hasProgramDates
:hasProgramDates rdf:type owl:ObjectProperty ;
                 rdfs:domain :Program ;
                 rdfs:range <http://www.w3.org/2006/time#TemporalEntity> ;
                 rdfs:comment "A date range for the start and end of a program"@en-us ;
                 rdfs:label "hasProgramDates"@en-us .


###  http://ocean-data.org/schema/hasProject
:hasProject rdf:type owl:ObjectProperty ;
            rdfs:domain :Program ;
            rdfs:range :Project ;
            rdfs:comment "A Project that is a part of this Program"@en-us ;
            rdfs:label "hasProject"@en-us .


###  http://ocean-data.org/schema/hasProjectDates
:hasProjectDates rdf:type owl:ObjectProperty ;
                 rdfs:domain :Project ;
                 rdfs:range <http://www.w3.org/2006/time#TemporalEntity> ;
                 rdfs:comment "A date range for the start and end of a project"@en-us ;
                 rdfs:label "hasProjectDates"@en-us .


###  http://ocean-data.org/schema/hasRole
:hasRole rdf:type owl:ObjectProperty ;
         rdfs:domain :Agent ;
         rdfs:range :Role .


###  http://ocean-data.org/schema/hasUnitOfMeasure
:hasUnitOfMeasure rdf:type owl:ObjectProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :DatasetMonitoredProperty
                                              :MonitoredProperty
                                            )
                              ] ;
                  rdfs:range :UnitOfMeasure ;
                  rdfs:comment "The unit of Measure used for a certain MonitoredProperty. In the case of MonitoredProperty where these are conceptual, the unit of measure is considered to the default, or convention, for that property. For DatasetMonitoredProperty, the unit of measure is what was actually used within the Dataset."@en-us ;
                  rdfs:label "hasUnitOfMeasure"@en-us .


###  http://ocean-data.org/schema/isArchivedBy
:isArchivedBy rdf:type owl:ObjectProperty ;
              rdfs:domain :CreativeWork ;
              rdfs:range :ArchiveRecord ;
              rdfs:comment "An ArchiveRecord that stores soem CreativeWork"@en-us ;
              rdfs:label "isArchivedBy"@en-us .


###  http://ocean-data.org/schema/isDeployed
:isDeployed rdf:type owl:ObjectProperty ;
            owl:inverseOf :ofPlatform ;
            rdfs:domain :Platform ;
            rdfs:range :Deployment ;
            rdfs:comment "This Platform was deployed on the Deployment"@en-us ,
                         "fix" ;
            rdfs:label "isDeployed"@en-us .


###  http://ocean-data.org/schema/isInstanceOf
:isInstanceOf rdf:type owl:ObjectProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :DatasetInstrument
                                          :DatasetMonitoredProperty
                                        )
                          ] ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( :Instrument
                                         :MonitoredProperty
                                       )
                         ] ;
              rdfs:comment "Defines the concept for which the subject is a realized instance."@en-us ;
              rdfs:label "isInstanceOf"@en-us .


###  http://ocean-data.org/schema/lastKnownAffiliation
:lastKnownAffiliation rdf:type owl:ObjectProperty ;
                      rdfs:domain :Person ;
                      rdfs:range :Organization ;
                      rdfs:comment " The last known organization, or affiliation, for a Person."@en-us ;
                      rdfs:label "lastKnownAffiliation"@en-us .


###  http://ocean-data.org/schema/matches
:matches rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf <http://www.w3.org/2004/02/skos/core#exactMatch> ;
         rdf:type owl:SymmetricProperty ;
         rdfs:domain owl:Thing ;
         rdfs:range owl:Thing ;
         rdfs:comment "A symmetric match for linking between instance data of ODO and coreferences in external Linked Data sets.  equivalent to skos:exactMatch and owl:sameAs without the reasoning that makes them inherit each other's properties."@en-us ;
         rdfs:label "matches"@en-us .


###  http://ocean-data.org/schema/obsoletedBy
:obsoletedBy rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain owl:Thing ;
             rdfs:range owl:Thing ;
             rdfs:comment "Defines the resource that has replaced this resource."@en-us ;
             rdfs:label "obsoletedBy"@en-us .


###  http://ocean-data.org/schema/ofPlatform
:ofPlatform rdf:type owl:ObjectProperty ;
            rdfs:domain :Deployment ;
            rdfs:range :Platform ;
            rdfs:comment "A deployment of a certain Platform"@en-us ;
            rdfs:label "ofPlatforn"@en-us .


###  http://ocean-data.org/schema/performedBy
:performedBy rdf:type owl:ObjectProperty ;
             rdfs:domain :Role ;
             rdfs:range :Agent ;
             rdfs:comment "The Agent performing a certain Role in a research activity or on a research product."@en-us ;
             rdfs:label "performedBy"@en-us .


###  http://ocean-data.org/schema/performedRole
:performedRole rdf:type owl:ObjectProperty ;
               rdfs:domain :Agent ;
               rdfs:range :Role ;
               rdfs:comment "The roles performed by a certain Agent"@en-us ;
               rdfs:label "performedRole"@en-us .


###  http://ocean-data.org/schema/producedWork
:producedWork rdf:type owl:ObjectProperty ;
              rdfs:domain :ResearchActivity ;
              rdfs:range :CreativeWork ;
              rdfs:comment "The research activity produced the research product."@en-us ;
              rdfs:label "producedWork"@en-us .


###  http://ocean-data.org/schema/providesAward
:providesAward rdf:type owl:ObjectProperty ;
               rdfs:domain :FundingSource ;
               rdfs:range :Award ;
               rdfs:comment "A FundingSource provides an Award in support of some research activity."@en-us ;
               rdfs:label "providesAward"@en-us .


###  http://ocean-data.org/schema/relatedWork
:relatedWork rdf:type owl:ObjectProperty ;
             rdfs:domain :CreativeWork ;
             rdfs:range :CreativeWorkRelation ;
             rdfs:comment "A relationship between two Creative Works that details how they are related"@en-us ;
             rdfs:label "relatedWork"@en-us .


###  http://ocean-data.org/schema/relationshipType
:relationshipType rdf:type owl:ObjectProperty ;
                  rdfs:domain :CreativeWorkRelation ;
                  rdfs:range :CreativeWorkRelationType ;
                  rdfs:comment "The type of relation between two Creative Works."@en-us ;
                  rdfs:label "relationshipType"@en-us .


###  http://ocean-data.org/schema/roleAbout
:roleAbout rdf:type owl:ObjectProperty ;
           rdfs:domain :Role ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Award
                                      :Dataset
                                      :Deployment
                                      :Organization
                                      :ResearchActivity
                                    )
                      ] ;
           rdfs:comment "The research activity or product of which this role is about. Was using dc:subject"@en-us ;
           rdfs:label "roleAbout"@en-us .


###  http://ocean-data.org/schema/roleType
:roleType rdf:type owl:ObjectProperty ;
          rdfs:domain :Role ;
          rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty <http://www.w3.org/2004/02/skos/core#member> ;
                                              owl:allValuesFrom :AgentRole
                                            ]
                                          ) ;
                       rdf:type owl:Class
                     ] ;
          rdfs:comment "The type of Role"@en-us ;
          rdfs:label "roleType"@en-us .


###  http://ocean-data.org/schema/serializes
:serializes rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :DataFile ;
            rdfs:range :Dataset ;
            rdfs:comment "The Dataset this datafile is a representation of"@en-us ;
            rdfs:label "serializes"@en-us .


###  http://ocean-data.org/schema/storesValuesFor
:storesValuesFor rdf:type owl:ObjectProperty ;
                 rdfs:domain :Dataset ;
                 rdfs:range :DatasetMonitoredProperty ;
                 rdfs:comment "This dataset stores values for the specified DatasetMonitoredProperty"@en-us ;
                 rdfs:label "storesValuesFor"@en-us .


###  http://ocean-data.org/schema/usesIdentifierScheme
:usesIdentifierScheme rdf:type owl:ObjectProperty ;
                      rdfs:domain :Identifier ;
                      rdfs:range :IdentifierScheme ;
                      rdfs:comment "The identifier scheme used by some identifer. this scheme defines the space in which the identifier was minted and should be ultimately have a unqiue value within."@en-us ;
                      rdfs:label "usesIdentifierScheme"@en-us .


###  http://www.w3.org/2004/02/skos/core#exactMatch
<http://www.w3.org/2004/02/skos/core#exactMatch> rdf:type owl:ObjectProperty .


###  http://www.w3.org/2004/02/skos/core#member
<http://www.w3.org/2004/02/skos/core#member> rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#generated
<http://www.w3.org/ns/prov#generated> rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#onBehalfOf
<http://www.w3.org/ns/prov#onBehalfOf> rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://ocean-data.org/schema/abstract
:abstract rdf:type owl:DatatypeProperty ;
          rdfs:domain :Dataset ;
          rdfs:range xsd:string ;
          rdfs:comment "A description of some research activity or research product."@en-us ;
          rdfs:label "abstract"@en-us .


###  http://ocean-data.org/schema/alternateName
:alternateName rdf:type owl:DatatypeProperty ;
               rdfs:domain :Platform ;
               rdfs:range xsd:string ;
               rdfs:comment "An alternate name for some thing"@en-us ;
               rdfs:label "alternateName"@en-us .


###  http://ocean-data.org/schema/archivedDate
:archivedDate rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :ArchiveRecord ;
              rdfs:range xsd:date ;
              rdfs:comment "The date an ArchiveRecord was minted"@en-us ;
              rdfs:label "archivedDate"@en-us .


###  http://ocean-data.org/schema/bytesize
:bytesize rdf:type owl:DatatypeProperty ;
          rdfs:domain :DigitalDocument ;
          rdfs:range xsd:long ;
          rdfs:comment "The bytesize of some DigitalDocument"@en-us ;
          rdfs:label "bytesize"@en-us .


###  http://ocean-data.org/schema/checksum
:checksum rdf:type owl:DatatypeProperty ;
          rdfs:domain :DigitalDocument ;
          rdfs:range xsd:string ;
          rdfs:comment "The checksum value of some DigitalDocument for some checksum algorithm"@en-us ;
          rdfs:label "checksum"@en-us .


###  http://ocean-data.org/schema/citationText
:citationText rdf:type owl:DatatypeProperty ;
              rdfs:domain :CreativeWork ;
              rdfs:range xsd:string ;
              rdfs:comment "The citation text for a Creative Work."@en-us ;
              rdfs:label "citationText"@en-us .


###  http://ocean-data.org/schema/conversionDescription
