@prefix cpm: <http://purl.org/voc/cpm#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcite: <http://purl.org/spar/datacite/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geos: <http://www.opengis.net/def/geosparql/> .
@prefix odo: <http://ocean-data.org/schema/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

odo: a owl:Ontology ;
    rdfs:label "Ocean Data Ontology"^^xsd:string ;
    dc:description "An ontology for describing ocean data" ;
    dcterms:issued "2017-03-01"^^<xsd:date> ;
    dcterms:source "https://github.com/BCODMO/Ocean-Data-Ontology"^^<xsd:anyURI> ;
    owl:imports foaf: ;
    owl:versionInfo "0.9.0" .

odo:Aircraft a owl:Class ;
    rdfs:label "Aircraft"^^xsd:string ;
    rdfs:subClassOf odo:Vehicle .

odo:AutonomousUnderwaterVehicle a owl:Class ;
    rdfs:label "AUV"^^xsd:string ;
    rdfs:subClassOf odo:UnmannedSubmarine .

odo:Cruise a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue odo:Vessel ;
            owl:onProperty odo:ofPlatform ],
        odo:Deployment .

odo:Dock a owl:Class ;
    rdfs:label "Dock"^^xsd:string ;
    rdfs:subClassOf odo:StationaryPlatform .

odo:Island a owl:Class ;
    rdfs:label "Island"^^xsd:string ;
    rdfs:subClassOf odo:StationaryPlatform .

odo:Laboratory a owl:Class ;
    rdfs:label "Laboratory"^^xsd:string ;
    rdfs:subClassOf odo:StationaryPlatform .

odo:MannedSubmarine a owl:Class ;
    rdfs:label "Manned Submarine"^^xsd:string ;
    rdfs:subClassOf odo:Submarine .

odo:Mooring a owl:Class ;
    rdfs:label "Mooring"^^xsd:string ;
    rdfs:subClassOf odo:StationaryPlatform .

odo:RemotelyOperatedVehicle a owl:Class ;
    rdfs:label "ROV"^^xsd:string ;
    rdfs:subClassOf odo:UnmannedSubmarine .

odo:StationaryVessel a owl:Class ;
    rdfs:label "Stationary Vessel"^^xsd:string ;
    rdfs:subClassOf odo:StationaryPlatform,
        odo:Vessel .

odo:TowedVehicle a owl:Class ;
    rdfs:label "Towed Vehicle"^^xsd:string ;
    rdfs:subClassOf odo:Vehicle .

odo:citationText a owl:DatatypeProperty ;
    rdfs:comment "The citation text for a Creative Work."^^xsd:string ;
    rdfs:domain odo:CreativeWork ;
    rdfs:range xsd:string .

odo:conversionDescription a owl:DatatypeProperty ;
    rdfs:comment "Describes how the values can be converted." ;
    rdfs:domain odo:DatasetMonitoredProperty ;
    rdfs:range xsd:string .

odo:crossrefRelation a owl:DatatypeProperty ;
    rdfs:comment "The type of relationship defined by Crossref."^^xsd:string ;
    rdfs:domain odo:CreativeWorkRelation ;
    rdfs:range xsd:string .

odo:crossrefRelationType a owl:ObjectProperty ;
    rdfs:comment "Either it's an inter-relation or an intra-relation depending on if its the same work or not."^^xsd:string ;
    rdfs:domain odo:CreativeWorkRelation ;
    rdfs:range [ a owl:DataRange ;
            owl:oneOf [ a rdf:List ;
                    rdf:first ""^^odo:CrossrefInterRelation ;
                    rdf:rest [ a rdf:List ;
                            rdf:first ""^^odo:CrossrefIntraRelation ] ] ] .

odo:currentState a owl:ObjectProperty ;
    rdfs:comment "A term describing the current state of the dataset."^^xsd:string ;
    rdfs:domain odo:Dataset ;
    rdfs:range skos:Concept .

odo:dataFile a owl:DatatypeProperty ;
    rdfs:domain odo:Dataset ;
    rdfs:range owl:Thing .

odo:dataFileLandingPage a owl:DatatypeProperty ;
    rdfs:domain odo:Dataset ;
    rdfs:range owl:Thing .

odo:datasetType a owl:ObjectProperty ;
    rdfs:comment "The type of dataset."^^xsd:string ;
    rdfs:domain odo:Dataset ;
    rdfs:range skos:Concept .

odo:featureOfInterest a owl:ObjectProperty ;
    rdfs:label "Links an odo:MonitoredProperty to the feature, and from there the geometry, of interest"^^xsd:string ;
    rdfs:domain odo:MonitoredProperty ;
    rdfs:range geos:Feature ;
    rdfs:subPropertyOf skos:related .

odo:forOrganization a owl:ObjectProperty ;
    rdfs:domain odo:Role ;
    rdfs:range foaf:Organization .

odo:fromDeployment a owl:ObjectProperty ;
    rdfs:domain odo:DeploymentDataset ;
    rdfs:range odo:Deployment .

odo:fromInstrument a owl:ObjectProperty ;
    rdfs:comment "This dataset was collected from an instrument that is located on the platform during this deployment."^^xsd:string ;
    rdfs:domain odo:DeploymentDatasetCollection ;
    rdfs:range odo:Instrument .

odo:hasAcquisitionDescription a owl:DatatypeProperty ;
    rdfs:domain odo:Dataset ;
    rdfs:subPropertyOf dcterms:description .

odo:hasAcronym a owl:DatatypeProperty ;
    rdfs:domain foaf:Organization ;
    rdfs:range xsd:string .

odo:hasAffiliation a owl:ObjectProperty ;
    rdfs:domain foaf:Person ;
    rdfs:range foaf:Organization .

odo:hasAgentWithRole a owl:ObjectProperty ;
    rdfs:comment "Used by odo:Project, odo:Program, odo:Dataset, odo:Deployment classes"^^xsd:string ;
    rdfs:range odo:Role .

odo:hasAward a owl:ObjectProperty ;
    rdfs:comment "Used by odo:Dataset, odo:FundingSource classes"^^xsd:string ;
    rdfs:range odo:Award .

odo:hasAwardNumber a owl:DatatypeProperty ;
    rdfs:domain odo:Award ;
    rdfs:range xsd:string .

odo:hasAwardURL a owl:DatatypeProperty ;
    rdfs:domain odo:Award ;
    rdfs:subPropertyOf rdfs:seeAlso .

odo:hasBriefDescription a owl:DatatypeProperty ;
    rdfs:domain odo:DeploymentDatasetCollection ;
    rdfs:subPropertyOf dcterms:description .

odo:hasCoordinatedDeployment a owl:ObjectProperty ;
    rdfs:domain odo:Deployment ;
    rdfs:range odo:Deployment .

odo:hasCruiseTrajectory a owl:DatatypeProperty ;
    rdfs:domain odo:Deployment ;
    rdfs:range geos:Feature .

odo:hasDataManagementPlan a owl:DatatypeProperty ;
    rdfs:comment "Used by odo:Projectto specify the URL to the Project's Data Management Plan."^^xsd:string ;
    rdfs:domain odo:Project ;
    rdfs:subPropertyOf rdfs:seeAlso .

odo:hasDataset a owl:ObjectProperty ;
    rdfs:comment "Used by odo:DeploymentDataset, odo:Award, odo:Project classes"^^xsd:string ;
    rdfs:range odo:Dataset .

odo:hasDatasetURL a owl:DatatypeProperty ;
    rdfs:domain odo:Dataset ;
    rdfs:subPropertyOf rdfs:seeAlso .

odo:hasDeploymentDates a owl:ObjectProperty ;
    rdfs:comment "A date range for the start and end of a deployment"^^xsd:string ;
    rdfs:domain odo:Deployment ;
    rdfs:range time:TemporalEntity .

odo:hasDeploymentDescription a owl:DatatypeProperty ;
    rdfs:domain odo:Deployment ;
    rdfs:subPropertyOf dcterms:description .

odo:hasFundingSource a owl:ObjectProperty ;
    rdfs:comment "Used by odo:Award class, so not sure why the range hasn't been set on this property"^^xsd:string ;
    rdfs:range foaf:Organization .

odo:hasIdentifier a owl:ObjectProperty ;
    rdfs:comment "An identifier for a Creative Work."^^xsd:string ;
    rdfs:domain odo:CreativeWork ;
    rdfs:range dcite:Identifier ;
    rdfs:subPropertyOf dcite:hasIdentifier .

odo:hasMakeAndModel a owl:ObjectProperty ;
    rdfs:label "Links odo:Instrument to a skos:Concept of which the odo:Instrument is an instance"^^xsd:string ;
    rdfs:domain odo:Instrument ;
    rdfs:range skos:Concept ;
    rdfs:subPropertyOf rdfs:isDefinedBy,
        skos:related .

odo:hasMonitoredProperty a owl:ObjectProperty ;
    rdfs:domain odo:MonitoredProperty ;
    rdfs:range odo:Instrument .

odo:hasNoDataValue a owl:DatatypeProperty ;
    rdfs:comment "The value to expect in a Dataset when there was no data for the given record." ;
    rdfs:domain odo:MonitoredProperty ;
    rdfs:range xsd:string .

odo:hasOrganizationSubName a owl:DatatypeProperty ;
    rdfs:domain foaf:Organization ;
    rdfs:range xsd:string .

odo:hasParameter a owl:ObjectProperty ;
    rdfs:comment "Used by odo:Dataset class, so not sure why the range hasn't been set on this property. This property may have duplicate resources for now and these represent that there were 'n' number of parameters that measured the same conceptual property. This is the issue we are seeking to fix by modeling the local, colloquial term the PI used for this parameter, and then mapping it to this more conceptual understanding of that measured parameter."^^xsd:string ;
    rdfs:range odo:Parameter .

odo:hasParameterShortDescription a owl:DatatypeProperty ;
    rdfs:domain odo:MonitoredProperty ;
    rdfs:subPropertyOf dcterms:description .

odo:hasParameterShortName a owl:DatatypeProperty ;
    rdfs:domain odo:MonitoredProperty ;
    rdfs:range xsd:string .

odo:hasPlatformTitle a owl:DatatypeProperty ;
    rdfs:domain odo:Platform ;
    rdfs:range xsd:string .

odo:hasPlatformURL a owl:DatatypeProperty ;
    rdfs:domain odo:Platform ;
    rdfs:subPropertyOf rdfs:seeAlso .

odo:hasProcessingDescription a owl:DatatypeProperty ;
    rdfs:domain odo:Dataset ;
    rdfs:subPropertyOf dcterms:description .

odo:hasProgramDates a owl:ObjectProperty ;
    rdfs:comment "A date range for the start and end of a program"^^xsd:string ;
    rdfs:domain odo:Program ;
    rdfs:range time:TemporalEntity .

odo:hasProject a owl:ObjectProperty ;
    owl:inverseOf odo:hasParentProgram .

odo:hasProjectDates a owl:ObjectProperty ;
    rdfs:comment "A date range for the start and end of a project"^^xsd:string ;
    rdfs:domain odo:Project ;
    rdfs:range time:TemporalEntity .

odo:hasReportURL a owl:DatatypeProperty ;
    rdfs:domain odo:Deployment ;
    rdfs:subPropertyOf rdfs:seeAlso .

odo:hasRole a owl:ObjectProperty ;
    rdfs:domain foaf:Person ;
    rdfs:range odo:Role .

odo:hasSerialNumber a owl:DatatypeProperty ;
    rdfs:domain odo:Instrument ;
    rdfs:range xsd:string .

odo:hasSmallLogoURL a owl:DatatypeProperty ;
    rdfs:comment "Used by odo:Project, odo:Program classes"^^xsd:string ;
    rdfs:subPropertyOf foaf:logo .

odo:hasSynonym a owl:DatatypeProperty ;
    rdfs:domain odo:Deployment ;
    rdfs:range xsd:string .

odo:hasUnitOfMeasure a owl:ObjectProperty ;
    rdfs:label "hasUnitOfMeasure - Links odo:MonitoredProperty to a SKOS Concept defining the unit of measure"^^xsd:string ;
    rdfs:domain odo:MonitoredProperty ;
    rdfs:range odo:UnitOfMeasure ;
    rdfs:subPropertyOf skos:related ;
    owl:equivalentProperty cpm:opUnitOfMeasure .

odo:isDeployed a owl:ObjectProperty ;
    rdfs:domain odo:Platform ;
    rdfs:range odo:Deployment ;
    owl:inverseOf odo:ofPlatform .

odo:isInstanceOf a owl:ObjectProperty ;
    rdfs:label "Links odo instances (odo:Instrument and odo:MonitoredProperty) to skos:Concept"^^xsd:string ;
    rdfs:domain owl:Thing ;
    rdfs:range skos:Concept ;
    rdfs:subPropertyOf rdfs:isDefinedBy,
        skos:related .

odo:matches a owl:SymmetricProperty ;
    rdfs:comment "A symmetric match for linking between instance data of ODO and coreferences in external Linked Data sets"^^xsd:string ;
    rdfs:domain owl:Thing ;
    rdfs:range owl:Thing ;
    rdfs:subPropertyOf rdfs:seeAlso .

odo:obsoletedBy a owl:ObjectProperty ;
    rdfs:label "Defines the resource that has replaced this resource."^^xsd:string ;
    rdfs:domain skos:Concept ;
    rdfs:range skos:Concept .

odo:osprey_page a owl:DatatypeProperty ;
    rdfs:comment "Used by odo:Project, odo:Program, odo:Dataset, Person, Organization, odo:FundingSource, odo:Role, odo:Award, odo:Deployment, odo:Platform, odo:Instrument, odo:Parameter to specify the URL to the HTML metadata page at www.bco-dmo.org"^^xsd:string ;
    rdfs:subPropertyOf rdfs:seeAlso .

odo:performedBy a owl:ObjectProperty ;
    rdfs:domain odo:Role ;
    rdfs:range foaf:Person .

odo:r2r_page a owl:DatatypeProperty ;
    rdfs:comment "Used by odo:Cruise, but I don't thnk we use this anymore. Instead, we link to R2R cruises with owl:sameAs or odo:matches"^^xsd:string ;
    rdfs:subPropertyOf rdfs:seeAlso .

odo:relatedWork a owl:ObjectProperty ;
    rdfs:comment "A related work."^^xsd:string ;
    rdfs:domain odo:CreativeWork ;
    rdfs:range skos:CreativeWork .

odo:relationshipType a owl:ObjectProperty ;
    rdfs:comment "The type of relation between two Creative Works."^^xsd:string ;
    rdfs:domain odo:CreativeWork ;
    rdfs:range odo:CreativeWorkRelation .

odo:requiresConversion a owl:DatatypeProperty ;
    rdfs:comment "The values in the dataset for this property need to be converted." ;
    rdfs:domain odo:DatasetMonitoredProperty ;
    rdfs:range xsd:boolean .

odo:storesValuesFor a owl:ObjectProperty ;
    rdfs:domain odo:Dataset ;
    rdfs:range odo:MonitoredProperty .

odo:versionLabel a owl:DatatypeProperty ;
    rdfs:comment "An string signifying the version of the dataset."^^xsd:string ;
    rdfs:domain odo:Dataset ;
    rdfs:range xsd:string .

odo:wktLiteral a owl:DatatypeProperty ;
    rdfs:comment "A WKT representation of a geometry to use when avoiding Virtuoso's geometry validation."^^xsd:string ;
    rdfs:domain owl:Thing ;
    rdfs:range geos:wktLiteral ;
    rdfs:subPropertyOf geos:wktLiteral .

<odo:CrossrefInterRelation> a owl:NamedIndividual ;
    rdfs:label "Crossref Inter-relation"@en-US ;
    rdfs:comment "A relationship between two different creative works."@en-US .

<odo:CrossrefIntraRelation> a owl:NamedIndividual ;
    rdfs:label "Crossref Intra-relation"@en-US ;
    rdfs:comment "A relationship between two variations of the same creative work."@en-US .

odo:BcoDmoDataManagerRole a owl:Class ;
    rdfs:label "BCO-DMO Data Manager"^^xsd:string ;
    rdfs:subClassOf odo:Role ;
    owl:disjointWith odo:AnalystRole,
        odo:CaptainRole,
        odo:ChiefScientistRole,
        odo:Co-ChiefScientistRole,
        odo:Co-PrincipalInvestigatorRole,
        odo:ContactRole,
        odo:CuratorRole,
        odo:DataManagerRole,
        odo:DataPublisherRole,
        odo:GuestRole,
        odo:InternationalCollaboratorRole,
        odo:LeadPrincipalInvestigatorRole,
        odo:OriginatorRole,
        odo:PrincipalInvestigatorRole,
        odo:ProgramManagerRole,
        odo:ProjectCoordinatorRole,
        odo:ScientistRole,
        odo:StudentRole,
        odo:TechnicianRole,
        odo:UnknownRole .

odo:SubContractAward a owl:Class ;
    rdfs:label "Sub-Contract Award"^^xsd:string ;
    rdfs:subClassOf odo:Award ;
    owl:disjointWith odo:ContractAward,
        odo:GrantAward .

odo:fromCollection a owl:ObjectProperty ;
    rdfs:label "fromCollection"^^xsd:string ;
    owl:inverseOf odo:hasInventory .

odo:hasParentProgram a owl:ObjectProperty ;
    rdfs:domain odo:Project ;
    rdfs:range odo:Program .

odo:releaseDate a owl:DatatypeProperty ;
    rdfs:comment "The date the dataset will be available. Useful for reporting when a restricted dataset will become available."^^xsd:string ;
    rdfs:domain odo:Dataset .

odo:restricted a owl:DatatypeProperty ;
    rdfs:comment "Is the dataset restricted for access by the public."^^xsd:string ;
    rdfs:domain odo:Dataset ;
    rdfs:range xsd:boolean .

odo:validated a owl:DatatypeProperty ;
    rdfs:comment "Has the dataset been validated by the PI."^^xsd:string ;
    rdfs:domain odo:Dataset ;
    rdfs:range xsd:boolean .

odo:ContractAward a owl:Class ;
    rdfs:label "Contract Award"^^xsd:string ;
    rdfs:subClassOf odo:Award ;
    owl:disjointWith odo:GrantAward .

odo:DatasetMonitoredProperty a owl:Class ;
    rdfs:label "Dataset Property"^^xsd:string ;
    rdfs:subClassOf odo:MonitoredProperty ;
    skos:defintion "odo:DatasetMonitoredProperty is an instance of a odo:MonitoredProperty but with project-specific label and definition known by the researcher." .

odo:DeploymentDatasetCollection a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom odo:DeploymentDataset ;
            owl:onProperty odo:hasInventory ],
        odo:DatasetCollection .

odo:ProjectCoordinatorRole a owl:Class ;
    rdfs:label "Project Coordinator"^^xsd:string ;
    rdfs:subClassOf [ rdf:Resource "http://ocean-data.org/schema/Role" ] ;
    owl:disjointWith odo:AnalystRole,
        odo:CaptainRole,
        odo:ChiefScientistRole,
        odo:Co-ChiefScientistRole,
        odo:Co-PrincipalInvestigatorRole,
        odo:ContactRole,
        odo:CuratorRole,
        odo:DataManagerRole,
        odo:DataPublisherRole,
        odo:GuestRole,
        odo:InternationalCollaboratorRole,
        odo:LeadPrincipalInvestigatorRole,
        odo:OriginatorRole,
        odo:PrincipalInvestigatorRole,
        odo:ProgramManagerRole,
        odo:ScientistRole,
        odo:StudentRole,
        odo:TechnicianRole,
        odo:UnknownRole .

odo:Submarine a owl:Class ;
    rdfs:label "Submarine"^^xsd:string ;
    rdfs:subClassOf odo:Vehicle .

odo:UnmannedSubmarine a owl:Class ;
    rdfs:label "Unmanned Submarine"^^xsd:string ;
    rdfs:subClassOf odo:Submarine .

odo:Vessel a owl:Class ;
    rdfs:label "Vessel"^^xsd:string ;
    rdfs:subClassOf odo:Vehicle .

odo:hasInventory a owl:ObjectProperty ;
    rdfs:label "hasInventory"^^xsd:string ;
    rdfs:domain odo:DatasetCollection ;
    rdfs:range odo:Dataset ;
    owl:inverseOf odo:fromCollection .

odo:ofPlatform a owl:ObjectProperty ;
    rdfs:domain odo:Deployment ;
    rdfs:range odo:Platform .

odo:DatasetCollection a owl:Class ;
    rdfs:label "Dataset Collection"^^xsd:string ;
    rdfs:subClassOf odo:Dataset ;
    owl:disjointWith odo:DeploymentDataset,
        odo:LaboratoryDataset .

odo:GrantAward a owl:Class ;
    rdfs:label "Grant Award"^^xsd:string ;
    rdfs:subClassOf odo:Award .

odo:InternationalCollaboratorRole a owl:Class ;
    rdfs:label "International Collaborator"^^xsd:string ;
    rdfs:subClassOf odo:Role ;
    owl:disjointWith odo:AnalystRole,
        odo:CaptainRole,
        odo:ChiefScientistRole,
        odo:Co-ChiefScientistRole,
        odo:Co-PrincipalInvestigatorRole,
        odo:ContactRole,
        odo:CuratorRole,
        odo:DataManagerRole,
        odo:DataPublisherRole,
        odo:GuestRole,
        odo:LeadPrincipalInvestigatorRole,
        odo:OriginatorRole,
        odo:PrincipalInvestigatorRole,
        odo:ProgramManagerRole,
        odo:ScientistRole,
        odo:StudentRole,
        odo:TechnicianRole,
        odo:UnknownRole .

odo:LaboratoryDataset a owl:Class ;
    rdfs:label "Laboratory Dataset"^^xsd:string ;
    rdfs:subClassOf odo:Dataset .

odo:Synthesis a owl:Class ;
    rdfs:label "Synthesis"^^xsd:string ;
    rdfs:subClassOf odo:Platform ;
    owl:disjointWith odo:Drifter,
        odo:Float,
        odo:Glider,
        odo:Model,
        odo:Satellite .

odo:DeploymentDataset a owl:Class ;
    rdfs:label "Deployment Dataset"^^xsd:string ;
    rdfs:comment "A deployment dataset is a dataset that is collected on a specific deployment of a platform from a specific instrument on that platform."^^xsd:string ;
    rdfs:subClassOf odo:Dataset ;
    owl:disjointWith odo:LaboratoryDataset .

odo:Model a owl:Class ;
    rdfs:label "Model"^^xsd:string ;
    rdfs:subClassOf odo:Platform ;
    owl:disjointWith odo:Drifter,
        odo:Float,
        odo:Glider,
        odo:Satellite .

odo:UnknownRole a owl:Class ;
    rdfs:label "Unknown"^^xsd:string ;
    rdfs:subClassOf odo:Role ;
    owl:disjointWith odo:AnalystRole,
        odo:CaptainRole,
        odo:ChiefScientistRole,
        odo:Co-ChiefScientistRole,
        odo:Co-PrincipalInvestigatorRole,
        odo:ContactRole,
        odo:CuratorRole,
        odo:DataManagerRole,
        odo:DataPublisherRole,
        odo:GuestRole,
        odo:LeadPrincipalInvestigatorRole,
        odo:OriginatorRole,
        odo:PrincipalInvestigatorRole,
        odo:ProgramManagerRole,
        odo:ScientistRole,
        odo:StudentRole,
        odo:TechnicianRole .

foaf:Person a owl:Class ;
    rdfs:label "Person"^^xsd:string ;
    owl:disjointWith odo:Award,
        odo:Dataset,
        odo:Deployment,
        odo:FundingSource,
        odo:Instrument,
        odo:MonitoredProperty,
        odo:Platform,
        odo:Program,
        odo:Project,
        odo:UnitOfMeasure,
        foaf:Organization .

odo:CreativeWork a owl:Class ;
    rdfs:label "Creative Work"^^xsd:string ;
    rdfs:subClassOf schema:CreativeWork .

odo:Program a owl:Class ;
    rdfs:label "Program"^^xsd:string ;
    rdfs:comment "A large-scale project that encompasses smaller Projects."^^xsd:string ;
    owl:disjointWith odo:Award,
        odo:Dataset,
        odo:Deployment,
        odo:FundingSource,
        odo:Instrument,
        odo:MonitoredProperty,
        odo:Platform,
        odo:UnitOfMeasure,
        foaf:Organization .

odo:ProgramManagerRole a owl:Class ;
    rdfs:label "Program Manager"^^xsd:string ;
    rdfs:subClassOf odo:Role ;
    owl:disjointWith odo:AnalystRole,
        odo:CaptainRole,
        odo:ChiefScientistRole,
        odo:Co-ChiefScientistRole,
        odo:Co-PrincipalInvestigatorRole,
        odo:ContactRole,
        odo:CuratorRole,
        odo:DataManagerRole,
        odo:DataPublisherRole,
        odo:GuestRole,
        odo:LeadPrincipalInvestigatorRole,
        odo:OriginatorRole,
        odo:PrincipalInvestigatorRole,
        odo:ScientistRole,
        odo:StudentRole,
        odo:TechnicianRole .

odo:Project a owl:Class ;
    rdfs:label "Project"^^xsd:string ;
    rdfs:comment "A funded, scientific objective that may produce Datasets."^^xsd:string ;
    rdfs:subClassOf foaf:Project ;
    owl:disjointWith odo:Award,
        odo:Dataset,
        odo:Deployment,
        odo:FundingSource,
        odo:Instrument,
        odo:MonitoredProperty,
        odo:Platform,
        odo:Program,
        odo:UnitOfMeasure,
        foaf:Organization .

odo:Satellite a owl:Class ;
    rdfs:label "Satellite"^^xsd:string ;
    rdfs:subClassOf odo:Platform ;
    owl:disjointWith odo:Drifter,
        odo:Float,
        odo:Glider .

odo:Vehicle a owl:Class ;
    rdfs:label "Vehicle"^^xsd:string ;
    rdfs:subClassOf odo:Platform ;
    owl:disjointWith odo:Drifter,
        odo:Float,
        odo:Glider,
        odo:Model,
        odo:Satellite,
        odo:StationaryPlatform,
        odo:Synthesis .

odo:ContactRole a owl:Class ;
    rdfs:label "Contact"^^xsd:string ;
    rdfs:subClassOf odo:Role ;
    owl:disjointWith odo:AnalystRole,
        odo:CaptainRole,
        odo:ChiefScientistRole,
        odo:Co-ChiefScientistRole,
        odo:Co-PrincipalInvestigatorRole,
        odo:CuratorRole,
        odo:DataManagerRole,
        odo:DataPublisherRole,
        odo:GuestRole,
        odo:LeadPrincipalInvestigatorRole,
        odo:OriginatorRole,
        odo:PrincipalInvestigatorRole,
        odo:ScientistRole,
        odo:StudentRole,
        odo:TechnicianRole .

odo:Drifter a owl:Class ;
    rdfs:label "Drifter"^^xsd:string ;
    rdfs:subClassOf odo:Platform ;
    owl:disjointWith odo:Float,
        odo:Glider .

odo:UnitOfMeasure a owl:Class ;
    rdfs:label "Unit of Measure"^^xsd:string ;
    rdfs:comment "A unit of measure"^^xsd:string ;
    rdfs:subClassOf skos:Concept ;
    owl:disjointWith odo:Award,
        odo:Dataset,
        odo:Deployment,
        odo:FundingSource,
        odo:Instrument,
