odo:Dataset a owl:Class ;
    rdfs:label "Dataset"^^xsd:string ;
    rdfs:comment "A super class for discovering any type of oceanographic dataset."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty odo:validated ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty odo:releaseDate ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty odo:restricted ],
        odo:CreativeWork ;
    owl:disjointWith odo:Award,
        foaf:Organization ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf ( odo:LaboratoryDataset odo:DeploymentDataset odo:DatasetCollection ) ] .

