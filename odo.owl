<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY odo "http://ocean-data.org/schema/">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
  <!ENTITY foaf "http://xmlns.com/foaf/0.1/">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY datacite "http://purl.org/spar/datacite/">
  <!ENTITY dc "http://purl.org/dc/elements/1.1/">
  <!ENTITY dcterms "http://purl.org/dc/terms/">
  <!ENTITY geos "http://www.opengis.net/def/geosparql/">
  <!ENTITY schema "http://schema.org/">
  <!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
  <!ENTITY time "http://www.w3.org/2006/time#">
  <!ENTITY cpm "http://purl.org/voc/cpm#">
]>
<rdf:RDF
    xmlns:owl="&owl;"
    xmlns:odo="&odo;"
    xmlns:xsd="&xsd;"
    xmlns:foaf="&foaf;"
    xmlns:rdf="&rdf;"
    xmlns:rdfs="&rdfs;"
    xmlns:datacite="&datacite;"
    xmlns:dc="&dc;"
    xmlns:dcterms="&dcterms;"
    xmlns:geos="&geos;"
    xmlns:schema="&schema;"
    xmlns:skos="&skos;"
    xmlns:time="&time;"
    xmlns:cpm="&cpm;"
    xml:base="&odo;">

  <owl:Ontology rdf:about="&odo;">
    <rdfs:label rdf:datatype="&xsd;string">Ocean Data Ontology</rdfs:label>
    <dc:description>An ontology for describing ocean data</dc:description>
    <owl:imports rdf:resource="&foaf;"/>
    <dcterms:source rdf:datatype="xsd:anyURI">https://github.com/BCODMO/Ocean-Data-Ontology</dcterms:source>
    <dcterms:issued rdf:datatype="xsd:date">2017-03-01</dcterms:issued>
    <owl:versionInfo>0.9.0</owl:versionInfo>
  </owl:Ontology>

  <!-- AFFILIATION -->
  <owl:Class rdf:about="&foaf;Organization">
    <rdfs:label rdf:datatype="&xsd;string">Organization</rdfs:label>
    <rdfs:comment rdf:datatype="&xsd;string">Organizations are used to keep track of Funding Sources and also a Person's affiliation.</rdfs:comment>
  </owl:Class>
  <owl:DatatypeProperty rdf:about="&odo;hasOrganizationSubName">
    <rdfs:range rdf:resource="&xsd;string"/>
    <rdfs:domain rdf:resource="&foaf;Organization"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="&odo;hasAcronym">
    <rdfs:range rdf:resource="&xsd;string"/>
    <rdfs:domain rdf:resource="&foaf;Organization"/>
  </owl:DatatypeProperty>


  <!-- AWARD -->
  <owl:Class rdf:about="&odo;Award">
    <rdfs:label rdf:datatype="&xsd;string">Award</rdfs:label>
    <rdfs:comment rdf:datatype="&xsd;string">A funding award provided by an Funding Source (Organization) to a Project. This Award may fund Datasets.</rdfs:comment>
    <owl:disjointWith rdf:resource="&foaf;Organization"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
	  <owl:Class rdf:about="&odo;GrantAward"/>
          <owl:Class rdf:about="&odo;ContractAward"/>
          <owl:Class rdf:about="&odo;SubContractAward"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:DatatypeProperty rdf:about="&odo;hasAwardNumber">
    <rdfs:range rdf:resource="&xsd;string"/>
    <rdfs:domain rdf:resource="&odo;Award"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="&odo;hasAwardURL">
    <rdfs:subPropertyOf rdf:resource="&rdfs;seeAlso"/>
    <rdfs:domain rdf:resource="&odo;Award"/>
  </owl:DatatypeProperty>

  <owl:Class rdf:about="&odo;GrantAward">
	<rdfs:label rdf:datatype="&xsd;string">Grant Award</rdfs:label>
    <rdfs:subClassOf rdf:resource="&odo;Award"/>
  </owl:Class>
  <owl:Class rdf:about="&odo;ContractAward">
    <rdfs:label rdf:datatype="&xsd;string">Contract Award</rdfs:label>
    <rdfs:subClassOf rdf:resource="&odo;Award"/>
    <owl:disjointWith rdf:resource="&odo;GrantAward"/>
  </owl:Class>
  <owl:Class rdf:about="&odo;SubContractAward">
    <rdfs:label rdf:datatype="&xsd;string">Sub-Contract Award</rdfs:label>
    <rdfs:subClassOf rdf:resource="&odo;Award"/>
    <owl:disjointWith rdf:resource="&odo;GrantAward"/>
    <owl:disjointWith rdf:resource="&odo;ContractAward"/>
  </owl:Class>
  
  <!-- CREATIVE WORK -->
  <owl:Class rdf:about="&odo;CreativeWork">
    <rdfs:label rdf:datatype="&xsd;string">Creative Work</rdfs:label>
    <rdfs:subClassOf rdf:resource="&schema;CreativeWork"/>
  </owl:Class>
  
  <owl:ObjectProperty rdf:about="&odo;relatedWork">
    <rdfs:domain rdf:resource="&odo;CreativeWork" />
    <rdfs:comment rdf:datatype="&xsd;string">A related work.</rdfs:comment>
    <rdfs:range rdf:resource="&skos;CreativeWork" />
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="&odo;relationshipType">
    <rdfs:domain rdf:resource="&odo;CreativeWork" />
    <rdfs:comment rdf:datatype="&xsd;string">The type of relation between two Creative Works.</rdfs:comment>
    <rdfs:range rdf:resource="&odo;CreativeWorkRelation" />
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="&odo;hasIdentifier">
    <rdfs:subPropertyOf rdf:resource="&datacite;hasIdentifier" />
    <rdfs:domain rdf:resource="&odo;CreativeWork" />
    <rdfs:comment rdf:datatype="&xsd;string">An identifier for a Creative Work.</rdfs:comment>
    <rdfs:range rdf:resource="&datacite;Identifier" />
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="&odo;citationText">
    <rdfs:comment rdf:datatype="&xsd;string">The citation text for a Creative Work.</rdfs:comment>
    <rdfs:domain rdf:resource="&odo;CreativeWork" />
    <rdfs:range rdf:resource="&xsd;string" />
  </owl:DatatypeProperty>

  <owl:Class rdf:About="&odo;CreativeWorkRelation">
    <rdfs:label rdf:datatype="&xsd;string">Creative Work Relationship</rdfs:label>
    <rdfs:subClassOf rdf:resource="&skos;Concept"/>
  </owl:Class>
  <owl:DatatypeProperty rdf:about="&odo;crossrefRelation">
    <rdfs:comment rdf:datatype="&xsd;string">The type of relationship defined by Crossref.</rdfs:comment>
    <rdfs:domain rdf:resource="&odo;CreativeWorkRelation" />
    <rdfs:range rdf:resource="&xsd;string" />
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="&odo;crossrefRelationType">
    <rdfs:comment rdf:datatype="&xsd;string">Either it's an inter-relation or an intra-relation depending on if its the same work or not.</rdfs:comment>
    <rdfs:domain rdf:resource="&odo;CreativeWorkRelation" />
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf>
          <rdf:List>
            <rdf:first rdf:datatype="&odo;CrossrefInterRelation" />
            <rdf:rest>
              <rdf:List>
                <rdf:first rdf:datatype="&odo;CrossrefIntraRelation" />
              </rdf:List>
            </rdf:rest>
          </rdf:List>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
  </owl:ObjectProperty>
  <owl:NamedIndividual rdf:about="odo:CrossrefInterRelation">
    <rdfs:label xml:lang="en-US">Crossref Inter-relation</rdfs:label>
    <rdfs:comment xml:lang="en-US">A relationship between two different creative works.</rdfs:comment>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="odo:CrossrefIntraRelation">
    <rdfs:label xml:lang="en-US">Crossref Intra-relation</rdfs:label>
    <rdfs:comment xml:lang="en-US">A relationship between two variations of the same creative work.</rdfs:comment>
  </owl:NamedIndividual>

  <!-- DATASETS -->
  <owl:Class rdf:about="&odo;Dataset">
    <rdfs:label rdf:datatype="&xsd;string">Dataset</rdfs:label>
    <rdfs:comment rdf:datatype="&xsd;string">A super class for discovering any type of oceanographic dataset.</rdfs:comment>
    <owl:disjointWith rdf:resource="&odo;Award"/>
    <owl:disjointWith rdf:resource="&foaf;Organization"/>
    <rdfs:subClassOf rdf:resource="&odo;CreativeWork"/>
    <rdfs:subClassOf>
    	<owl:Restriction>
          <owl:onProperty rdf:resource="&odo;restricted" />
          <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
        </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
    	<owl:Restriction>
          <owl:onProperty rdf:resource="&odo;releaseDate" />
          <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
        </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
    	<owl:Restriction>
          <owl:onProperty rdf:resource="&odo;validated" />
          <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
        </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
	  <owl:Class rdf:about="&odo;LaboratoryDataset"/>
          <owl:Class rdf:about="&odo;DeploymentDataset"/>
          <owl:Class rdf:about="&odo;DatasetCollection"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:DatatypeProperty rdf:about="&odo;hasDatasetURL">
    <rdfs:subPropertyOf rdf:resource="&rdfs;seeAlso"/>
    <rdfs:domain rdf:resource="&odo;Dataset"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="&odo;hasProcessingDescription">
    <rdfs:subPropertyOf rdf:resource="&dcterms;description"/>
    <rdfs:domain rdf:resource="&odo;Dataset"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="&odo;hasAcquisitionDescription">
    <rdfs:subPropertyOf rdf:resource="&dcterms;description"/>
    <rdfs:domain rdf:resource="&odo;Dataset"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="&odo;dataFile">
    <rdfs:domain rdf:resource="&odo;Dataset" />
    <rdfs:range rdf:resource="&owl;Thing" />
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="&odo;dataFileLandingPage">
    <rdfs:domain rdf:resource="&odo;Dataset" />
    <rdfs:range rdf:resource="&owl;Thing" />
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="&odo;restricted">
    <rdfs:domain rdf:resource="&odo;Dataset" />
    <rdfs:comment rdf:datatype="&xsd;string">Is the dataset restricted for access by the public.</rdfs:comment>
    <rdfs:range rdf:resource="&xsd;boolean"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="&odo;releaseDate">
    <rdfs:comment rdf:datatype="&xsd;string">The date the dataset will be available. Useful for reporting when a restricted dataset will become available.</rdfs:comment>
    <rdfs:domain rdf:resource="&odo;Dataset" />
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="&odo;versionLabel">
    <rdfs:domain rdf:resource="&odo;Dataset" />
    <rdfs:comment rdf:datatype="&xsd;string">An string signifying the version of the dataset.</rdfs:comment>
    <rdfs:range rdf:resource="&xsd;string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="&odo;validated">
    <rdfs:domain rdf:resource="&odo;Dataset" />
    <rdfs:comment rdf:datatype="&xsd;string">Has the dataset been validated by the PI.</rdfs:comment>
    <rdfs:range rdf:resource="&xsd;boolean"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="&odo;currentState">
    <rdfs:domain rdf:resource="&odo;Dataset" />
    <rdfs:comment rdf:datatype="&xsd;string">A term describing the current state of the dataset.</rdfs:comment>
    <rdfs:range rdf:resource="&skos;Concept" />
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="&odo;datasetType">
    <rdfs:domain rdf:resource="&odo;Dataset" />
    <rdfs:comment rdf:datatype="&xsd;string">The type of dataset.</rdfs:comment>
    <rdfs:range rdf:resource="&skos;Concept" />
  </owl:ObjectProperty>

  <owl:Class rdf:about="&odo;LaboratoryDataset">
    <rdfs:label rdf:datatype="&xsd;string">Laboratory Dataset</rdfs:label>
    <rdfs:subClassOf rdf:resource="&odo;Dataset"/>
  </owl:Class>

  <owl:Class rdf:about="&odo;DeploymentDataset">
    <rdfs:label rdf:datatype="&xsd;string">Deployment Dataset</rdfs:label>
    <rdfs:comment rdf:datatype="&xsd;string">A deployment dataset is a dataset that is collected on a specific deployment of a platform from a specific instrument on that platform.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="&odo;Dataset"/>
    <owl:disjointWith rdf:resource="&odo;LaboratoryDataset"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="&odo;fromDeployment">
    <rdfs:range rdf:resource="&odo;Deployment"/>
    <rdfs:domain rdf:resource="&odo;DeploymentDataset"/>
  </owl:ObjectProperty>

  <owl:Class rdf:about="&odo;DatasetCollection">
    <rdfs:label rdf:datatype="&xsd;string">Dataset Collection</rdfs:label>
    <rdfs:subClassOf rdf:resource="&odo;Dataset"/>
    <owl:disjointWith rdf:resource="&odo;LaboratoryDataset"/>
    <owl:disjointWith rdf:resource="&odo;DeploymentDataset"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="&odo;hasInventory">
    <rdfs:label rdf:datatype="&xsd;string">hasInventory</rdfs:label>
    <rdfs:range rdf:resource="&odo;Dataset"/>
    <rdfs:domain rdf:resource="&odo;DatasetCollection"/>
    <owl:inverseOf rdf:resource="&odo;fromCollection"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="&odo;fromCollection">
    <rdfs:label rdf:datatype="&xsd;string">fromCollection</rdfs:label>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="&odo;hasInventory"/>
    </owl:inverseOf>
  </owl:ObjectProperty>

  <owl:Class rdf:about="&odo;DeploymentDatasetCollection">
    <rdfs:subClassOf rdf:resource="&odo;DatasetCollection"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&odo;hasInventory"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="&odo;DeploymentDataset"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:DatatypeProperty rdf:about="&odo;hasBriefDescription">
    <rdfs:subPropertyOf rdf:resource="&dcterms;description"/>
    <rdfs:domain rdf:resource="&odo;DeploymentDatasetCollection"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="&odo;fromInstrument">
    <rdfs:range rdf:resource="&odo;Instrument"/>
    <rdfs:comment rdf:datatype="&xsd;string">This dataset was collected from an instrument that is located on the platform during this deployment.</rdfs:comment>
    <rdfs:domain rdf:resource="&odo;DeploymentDatasetCollection"/>
  </owl:ObjectProperty>



  <!-- DEPLOYMENT -->
  <owl:Class rdf:about="&odo;Deployment">
    <rdfs:label rdf:datatype="&xsd;string">Deployment</rdfs:label>
    <rdfs:comment rdf:datatype="&xsd;string">A platform is deployed during a specific duration of time over a specific spacial coverage. For example, a boat is a platform. That boat is doployed (sent on a cruise) in order to collect data.</rdfs:comment>
    <owl:disjointWith rdf:resource="&odo;Award"/>
    <owl:disjointWith rdf:resource="&odo;Dataset"/>
    <owl:disjointWith rdf:resource="&foaf;Organization"/>
  </owl:Class>
  <owl:DatatypeProperty rdf:about="&odo;hasReportURL">
    <rdfs:subPropertyOf rdf:resource="&rdfs;seeAlso"/>
    <rdfs:domain rdf:resource="&odo;Deployment"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="&odo;hasCoordinatedDeployment">
    <rdfs:range rdf:resource="&odo;Deployment"/>
    <rdfs:domain rdf:resource="&odo;Deployment"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="&odo;hasSynonym">
    <rdfs:domain rdf:resource="&odo;Deployment"/>
    <rdfs:range rdf:resource="&xsd;string"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="&odo;ofPlatform">
    <rdfs:range rdf:resource="&odo;Platform"/>
    <rdfs:domain rdf:resource="&odo;Deployment"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="&odo;hasDeploymentDescription">
    <rdfs:subPropertyOf rdf:resource="&dcterms;description"/>
    <rdfs:domain rdf:resource="&odo;Deployment"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="&odo;hasCruiseTrajectory">
    <rdfs:range rdf:resource="&geos;Feature"/>
    <rdfs:domain rdf:resource="&odo;Deployment"/>
  </owl:DatatypeProperty>
  <owl:Class rdf:about="&odo;Cruise">
   <rdfs:subClassOf rdf:resource="&odo;Deployment"/>
   <rdfs:subClassOf>
     <owl:Restriction>
       <owl:onProperty>
         <owl:ObjectProperty rdf:about="&odo;ofPlatform"/>
       </owl:onProperty>
       <owl:hasValue>
         <owl:Class rdf:about="&odo;Vessel"/>
       </owl:hasValue>
     </owl:Restriction>
   </rdfs:subClassOf>
 </owl:Class>


  <!-- FUNDING SOURCE -->
  <owl:Class rdf:about="&odo;FundingSource">
    <rdfs:label rdf:datatype="&xsd;string">Funding Source</rdfs:label>
    <rdfs:comment rdf:datatype="&xsd;string">An Organization that provides funding Awards to a Project.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="&foaf;Organization"/>
    <owl:disjointWith rdf:resource="&odo;Award"/>
    <owl:disjointWith rdf:resource="&odo;Dataset"/>
    <owl:disjointWith rdf:resource="&odo;Deployment"/>
  </owl:Class>


  <!-- INSTRUMENT -->
  <owl:Class rdf:about="&odo;Instrument">
    <rdfs:label rdf:datatype="&xsd;string">Instrument</rdfs:label>
    <skos:defintion>odo:Instrument is an instance of an NVS L22 Concept. Use odo:isInstanceOf to link the odo:Instrument to the L22 Concept's URI.</skos:defintion>
    <rdfs:subClassOf rdf:resource="&skos;Concept"/>
    <owl:disjointWith rdf:resource="&odo;Award"/>
    <owl:disjointWith rdf:resource="&odo;Dataset"/>
    <owl:disjointWith rdf:resource="&odo;Deployment"/>
    <owl:disjointWith rdf:resource="&odo;FundingSource"/>
    <owl:disjointWith rdf:resource="&foaf;Organization"/>
  </owl:Class>
  <owl:DatatypeProperty rdf:about="&odo;hasSerialNumber">
    <rdfs:range rdf:resource="&xsd;string"/>
     <rdfs:domain rdf:resource="&odo;Instrument"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="&odo;hasMakeAndModel">
    <rdfs:subPropertyOf rdf:resource="&skos;related" />
    <rdfs:subPropertyOf rdf:resource="&rdfs;isDefinedBy" />
    <rdfs:label rdf:datatype="&xsd;string">Links odo:Instrument to a skos:Concept of which the odo:Instrument is an instance</rdfs:label>
    <rdfs:domain rdf:resource="&odo;Instrument" />
    <rdfs:range rdf:resource="&skos;Concept" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="&odo;obsoletedBy">
    <rdfs:label rdf:datatype="&xsd;string">Defines the resource that has replaced this resource.</rdfs:label>
    <rdfs:domain rdf:resource="&skos;Concept" />
    <rdfs:range rdf:resource="&skos;Concept" />
  </owl:ObjectProperty>


  <!-- MONITORED PROPERTY -->
  <owl:Class rdf:about="&odo;MonitoredProperty">
    <rdfs:label rdf:datatype="&xsd;string">Monitored Property</rdfs:label>
    <skos:defintion>odo:MonitoredProperty is an instance of an NVS P01 Concept. Use odo:isInstanceOf to link the odo:MonitoredProperty to the P01 Concept's URI.</skos:defintion>
    <rdfs:subClassOf rdf:resource="&skos;Concept"/>
    <owl:disjointWith rdf:resource="&odo;Award"/>
    <owl:disjointWith rdf:resource="&odo;Dataset"/>
    <owl:disjointWith rdf:resource="&odo;Deployment"/>
    <owl:disjointWith rdf:resource="&odo;FundingSource"/>
    <owl:disjointWith rdf:resource="&odo;Instrument"/>
    <owl:disjointWith rdf:resource="&foaf;Organization"/>
  </owl:Class>
  <owl:Class rdf:about="&odo;UnitOfMeasure">
  	<rdfs:label rdf:datatype="&xsd;string">Unit of Measure</rdfs:label>
  	<rdfs:comment rdf:datatype="&xsd;string">A unit of measure</rdfs:comment>
  	<rdfs:subClassOf rdf:resource="&skos;Concept"/>
	<owl:disjointWith rdf:resource="&odo;Award"/>
    <owl:disjointWith rdf:resource="&odo;Dataset"/>
    <owl:disjointWith rdf:resource="&odo;Deployment"/>
    <owl:disjointWith rdf:resource="&odo;FundingSource"/>
    <owl:disjointWith rdf:resource="&odo;Instrument"/>
    <owl:disjointWith rdf:resource="&odo;MonitoredProperty"/>
    <owl:disjointWith rdf:resource="&foaf;Organization"/>
  </owl:Class>

  <owl:DatatypeProperty rdf:about="&odo;hasParameterShortDescription">
    <rdfs:subPropertyOf rdf:resource="&dcterms;description"/>
    <rdfs:domain rdf:resource="&odo;MonitoredProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="&odo;hasParameterShortName">
    <rdfs:range rdf:resource="&xsd;string"/>
    <rdfs:domain rdf:resource="&odo;MonitoredProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="&odo;hasNoDataValue">
    <rdfs:comment>The value to expect in a Dataset when there was no data for the given record.</rdfs:comment>
    <rdfs:range rdf:resource="&xsd;string"/>
    <rdfs:domain rdf:resource="&odo;MonitoredProperty"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="&odo;hasUnitOfMeasure">
    <rdfs:subPropertyOf rdf:resource="&skos;related" />
    <owl:equivalentProperty rdf:resource="&cpm;opUnitOfMeasure" />
    <rdfs:label rdf:datatype="&xsd;string">hasUnitOfMeasure - Links odo:MonitoredProperty to a SKOS Concept defining the unit of measure</rdfs:label>
    <rdfs:domain rdf:resource="&odo;MonitoredProperty" />
    <rdfs:range rdf:resource="&odo;UnitOfMeasure"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="&odo;featureOfInterest">
    <rdfs:label rdf:datatype="&xsd;string">Links an odo:MonitoredProperty to the feature, and from there the geometry, of interest</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="&skos;related" />
    <rdfs:domain rdf:resource="&odo;MonitoredProperty" />
    <rdfs:range rdf:resource="&geos;Feature"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="&odo;hasMonitoredProperty">
    <rdfs:range rdf:resource="&odo;Instrument"/>
    <rdfs:domain rdf:resource="&odo;MonitoredProperty"/>
   </owl:ObjectProperty>
   <owl:ObjectProperty rdf:about="&odo;storesValuesFor">
    <rdfs:domain rdf:resource="&odo;Dataset" />
    <rdfs:range rdf:resource="&odo;MonitoredProperty" />
   </owl:ObjectProperty>

   <!-- DATASET PARAMETER -->
   <owl:Class rdf:about="&odo;DatasetMonitoredProperty">
   	<rdfs:subClassOf rdf:resource="&odo;MonitoredProperty"/>
   	<rdfs:label rdf:datatype="&xsd;string">Dataset Property</rdfs:label>
    	<skos:defintion>odo:DatasetMonitoredProperty is an instance of a odo:MonitoredProperty but with project-specific label and definition known by the researcher.</skos:defintion>
