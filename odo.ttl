ns0:forOrganization
  a owl:ObjectProperty ;
  rdfs:range foaf:Organization ;
  rdfs:domain ns0:Role .

ns0:performedBy
  a owl:ObjectProperty ;
  rdfs:range foaf:Person ;
  rdfs:domain ns0:Role .

ns0:hasDataset
  a owl:ObjectProperty ;
  rdfs:range ns0:Dataset ;
  rdfs:comment "Used by odo:DeploymentDataset, odo:Award, odo:Project classes"^^xsd:string .

ns0:hasAward
  a owl:ObjectProperty ;
  rdfs:range ns0:Award ;
  rdfs:comment "Used by odo:Dataset, odo:FundingSource classes"^^xsd:string .

ns0:hasSmallLogoURL
  a owl:DatatypeProperty ;
  rdfs:subPropertyOf foaf:logo ;
  rdfs:comment "Used by odo:Project, odo:Program classes"^^xsd:string .

ns0:hasDataManagementPlan
  a owl:DatatypeProperty ;
  rdfs:subPropertyOf rdfs:seeAlso ;
  rdfs:comment "Used by odo:Projectto specify the URL to the Project's Data Management Plan."^^xsd:string ;
  rdfs:domain ns0:Project .

ns0:osprey_page
  a owl:DatatypeProperty ;
  rdfs:subPropertyOf rdfs:seeAlso ;
  rdfs:comment "Used by odo:Project, odo:Program, odo:Dataset, Person, Organization, odo:FundingSource, odo:Role, odo:Award, odo:Deployment, odo:Platform, odo:Instrument, odo:Parameter to specify the URL to the HTML metadata page at www.bco-dmo.org"^^xsd:string .

ns0:r2r_page
  a owl:DatatypeProperty ;
  rdfs:subPropertyOf rdfs:seeAlso ;
  rdfs:comment "Used by odo:Cruise, but I don't thnk we use this anymore. Instead, we link to R2R cruises with owl:sameAs or odo:matches"^^xsd:string .

ns0:hasAgentWithRole
  a owl:ObjectProperty ;
  rdfs:range ns0:Role ;
  rdfs:comment "Used by odo:Project, odo:Program, odo:Dataset, odo:Deployment classes"^^xsd:string .

ns0:hasFundingSource
  a owl:ObjectProperty ;
  rdfs:range foaf:Organization ;
  rdfs:comment "Used by odo:Award class, so not sure why the range hasn't been set on this property"^^xsd:string .

ns0:hasParameter
  a owl:ObjectProperty ;
  rdfs:range ns0:Parameter ;
  rdfs:comment "Used by odo:Dataset class, so not sure why the range hasn't been set on this property. This property may have duplicate resources for now and these represent that there were 'n' number of parameters that measured the same conceptual property. This is the issue we are seeking to fix by modeling the local, colloquial term the PI used for this parameter, and then mapping it to this more conceptual understanding of that measured parameter."^^xsd:string .

ns0:hasDeploymentDates
  a owl:ObjectProperty ;
  rdfs:range <http://www.w3.org/2006/time#TemporalEntity> ;
  rdfs:domain ns0:Deployment ;
  rdfs:comment "A date range for the start and end of a deployment"^^xsd:string .

ns0:hasProjectDates
  a owl:ObjectProperty ;
  rdfs:range <http://www.w3.org/2006/time#TemporalEntity> ;
  rdfs:domain ns0:Project ;
  rdfs:comment "A date range for the start and end of a project"^^xsd:string .

ns0:hasProgramDates
  a owl:ObjectProperty ;
  rdfs:range <http://www.w3.org/2006/time#TemporalEntity> ;
  rdfs:domain ns0:Program ;
  rdfs:comment "A date range for the start and end of a program"^^xsd:string .

ns0:isInstanceOf
  a owl:ObjectProperty ;
  rdfs:subPropertyOf skos:related, rdfs:isDefinedBy ;
  rdfs:label "Links odo instances (odo:Instrument and odo:MonitoredProperty) to skos:Concept"^^xsd:string ;
  rdfs:domain owl:Thing ;
  rdfs:range skos:Concept .

ns0:matches
  a owl:SymmetricProperty ;
  rdfs:subPropertyOf rdf:seeAlso ;
  rdfs:domain owl:Thing ;
  rdfs:range owl:Thing ;
  rdfs:comment "A symmetric match for linking between instance data of ODO and coreferences in external Linked Data sets"^^xsd:string .

ns0:wktLiteral
  a owl:DatatypeProperty ;
  rdfs:subPropertyOf <http://www.opengis.net/def/geosparql/wktLiteral> ;
  rdfs:domain owl:Thing ;
  rdfs:comment "A WKT representation of a geometry to use when avoiding Virtuoso's geometry validation."^^xsd:string .

[]
  a owl:Class ;
  rdfs:label "Creative Work Relationship"^^xsd:string ;
  rdfs:subClassOf skos:Concept .
